<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
    <bean name="goHome" id="goHomeService" class="com.example.spring.provider.service.GoHomeImpl"/>
    <bean name="goHomeController" id="goHomeController" class="com.example.spring.provider.service.GoHomeController"

    >
        <property name="go">
            <bean parent="goHomeService"></bean>
        </property>

    </bean>
    <bean id="myFactoryBean" class="com.example.spring.provider.service.MyBeanFactory"/>
</beans>